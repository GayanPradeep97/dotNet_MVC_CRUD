@{
}

@model List<StudentPortal.Models.Entities.Students>

<h1>Students</h1>

<table class="table">
    <thead>
        <tr>
            <th>Id</th>
            <th>Name</th>
            <th>Email</th>
            <th></th>
             <th></th>
        </tr>
    </thead>

    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Name</td>
                <td>@item.Email</td   
                 <td></td>
               <td>
                   <a class="btn btn-primary" asp-controller="Students" asp-action="Edit" asp-route-id="@item.Id"></a>
               </td>
        
            </tr>
            
        }
    </tbody>


</table>


